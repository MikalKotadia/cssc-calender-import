        "ts-loader": "^9.x.x",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.29.0",
        "webpack": "^5.x",
        "webpack-cli": "^5.x",
        "webpack-dev-server": "^4.x"
    }
}
