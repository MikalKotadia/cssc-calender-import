{
    "manifest_version": 3,
    "name": "CSSC Calender Import",
    "version": "1.0.0",
    "description": "A CSSC Game Calender Import Chrome Extension",
    "permissions": [
        "storage",
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.homebasehub.ca/cssc/view-registration/*?view[display]=schedule*"
            ],
            "js": [
                "content/index.js"
            ]
        }
    ],
    "action": {
        "default_popup": "popup/index.html"
    }
}
